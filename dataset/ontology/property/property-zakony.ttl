cs ;  .crime-prop:pravniKvalifikace_SekceZakona a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikace,    crime-prop:sekceZakona ;  .crime-prop:pravniKvalifikace_Paragraf a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikace,    crime-prop:paragraf ;  .crime-prop:pravniKvalifikace_Odstavec a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikace,    crime-prop:odstavec ;  .crime-prop:pravniKvalifikace_Pismeno a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikace,    crime-prop:pismeno ;  .crime-prop:pravniKvalifikaceTrestnehoCinu a rdf:Property ;  rdfs:subPropertyOf crime-prop:pravniKvalifikace ;  rdfs:domain crime-class:TrestnyCin ;   .crime-prop:pravniKvalifikaceTrestnehoCinu_SekceZakona a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTrestnehoCinu,    crime-prop:pravniKvalifikace_SekceZakona ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Paragraf a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTrestnehoCinu,    crime-prop:pravniKvalifikace_Paragraf ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Odstavec a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTrestnehoCinu,    crime-prop:pravniKvalifikace_Odstavec ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Pismeno a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTrestnehoCinu,    crime-prop:pravniKvalifikace_Pismeno ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Hlavni a rdf:Property ;  rdfs:subPropertyOf crime-prop:pravniKvalifikaceTrestnehoCinu ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Hlavni_SekceZakona a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTrestnehoCinu_Hlavni,    crime-prop:pravniKvalifikaceTrestnehoCinu_SekceZakona ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Hlavni_Paragraf a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTrestnehoCinu_Hlavni,    crime-prop:pravniKvalifikaceTrestnehoCinu_Paragraf ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Hlavni_Odstavec a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTrestnehoCinu_Hlavni,    crime-prop:pravniKvalifikaceTrestnehoCinu_Odstavec ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Hlavni_Pismeno a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTrestnehoCinu_Hlavni,    crime-prop:pravniKvalifikaceTrestnehoCinu_Pismeno ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Soubeh a rdf:Property ;  rdfs:subPropertyOf crime-prop:pravniKvalifikaceTrestnehoCinu ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Soubeh_SekceZakona a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTrestnehoCinu_Soubeh,    crime-prop:pravniKvalifikaceTrestnehoCinu_SekceZakona ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Soubeh_Paragraf a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTrestnehoCinu_Soubeh,    crime-prop:pravniKvalifikaceTrestnehoCinu_Paragraf ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Soubeh_Odstavec a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTrestnehoCinu_Soubeh,    crime-prop:pravniKvalifikaceTrestnehoCinu_Odstavec ;  .crime-prop:pravniKvalifikaceTrestnehoCinu_Soubeh_Pismeno a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTrestnehoCinu_Soubeh,    crime-prop:pravniKvalifikaceTrestnehoCinu_Pismeno ;  .             crime-prop:pravniKvalifikaceTsk a rdf:Property ;  rdfs:label		"ustanovení zákona kvalifikující TSK"@cs;	rdfs:comment    "Tato property se používá k záznamu odkazu na ustanovení zákona, které kvalifikuje TSK. Ustanovení zákona kvalifikuje TSK, pokud kvalifikuje některý z trestných činů zahrnutý pod dané TSK."@cs;    rdfs:subPropertyOf crime-prop:pravniKvalifikace ;  rdfs:domain crime-class:Tsk ;   .crime-prop:pravniKvalifikaceTsk_SekceZakona a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTsk,    crime-prop:pravniKvalifikace_Paragraf ;  .crime-prop:pravniKvalifikaceTsk_Paragraf a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTsk,    crime-prop:pravniKvalifikace_Paragraf ;  .crime-prop:pravniKvalifikaceTsk_Odstavec a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTsk,    crime-prop:pravniKvalifikace_Odstavec ;  .crime-prop:pravniKvalifikaceTsk_Pismeno a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceTsk,    crime-prop:pravniKvalifikace_Pismeno ;  .# Property vyjadřující odkaz na zákon, který chrání předmětcrime-prop:ochranaPodleZakona a rdf:Property ;	rdfs:label		"odkaz na úsek zákona chránící subjekt"@cs,		"a reference to a protecting law section"@en ;	rdfs:comment		"Tato property se používá na vyjádření skutečnosti, že předmět, který subjekt této property vyjadřuje, je chráněn podle určitého úseku zákona."@cs,		"This property is used to express a fact that an object, that the subject of this property represents, is protected by and according to a certain section of law."@en ;		rdfs:subPropertyOf crime-prop:zakon ;	.# Property odkazující úsek zákona, který vymezuje pojemcrime-prop:vymezenoVZakone a rdf:Property ;	rdfs:label		"odkaz na úsek zákona vymezující pojem"@cs,		"a reference to a defining law section"@en ;	rdfs:comment		"Tato property se používá na vyjádření skutečnosti, že pojem, který je reprezentován subjektem této property, je vymezen v určitém úseku zákona."@cs,		"This property is used to express a fact that a concept, that the subject of this property represents, is defined by a certain section of law."@en ;		rdfs:subPropertyOf crime-prop:zakon ;	.crime-prop:pobytNaUzemiCRPodle a rdf:Property ;	rdfs:subPropertyOf crime-prop:pramenPrava ;	.crime-prop:pobytNaUzemiCRPodleZakona a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pobytNaUzemiCRPodle,    crime-prop:zakon ;	.crime-prop:pobytNaUzemiCRPodleVyhlasky a rdf:Property ;	rdfs:subPropertyOf    crime-prop:pobytNaUzemiCRPodle,    crime-prop:vyhlaska ;	.crime-prop:pravniKvalifikaceFormyHospodarskeTrestneCinnosti a rdf:Property ;  rdfs:subPropertyOf crime-prop:pravniKvalifikace ;  rdfs:domain crime-class:FormaHospodarskeTrestneCinnosti ;   .crime-prop:pravniKvalifikaceFormyHospodarskeTrestneCinnosti_Paragraf a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceFormyHospodarskeTrestneCinnosti,    crime-prop:pravniKvalifikace_Paragraf ;  .crime-prop:pravniKvalifikaceFormyHospodarskeTrestneCinnosti_Odstavec a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceFormyHospodarskeTrestneCinnosti,    crime-prop:pravniKvalifikace_Odstavec ;  .crime-prop:pravniKvalifikaceFormyHospodarskeTrestneCinnosti_Pismeno a rdf:Property ;  rdfs:subPropertyOf    crime-prop:pravniKvalifikaceFormyHospodarskeTrestneCinnosti,    crime-prop:pravniKvalifikace_Pismeno ;  .# --------------------------- ÚKON V TRESTNÍM ŘÍZENÍ ----------------------------crime-prop:typUkonuVTrestnimRizeni a rdf:Property ;  rdfs:label "typ úkonu v trestním řízení"@cs ;  rdfs:comment "Pro konkrétní úkon v trestním řízení je tímto zaznamenán jeho typ. Konkrétní úkon v trestním řízení může mít více typů."@cs ;  rdfs:domain crime-class:UkonVTrestnimRizeni ;  rdfs:range crime-class:TypUkonuVTrestnimRizeni ;  .crime-prop:ukonVTrestnimRizeni a rdf:Property ;  rdfs:label "úkon v trestním řízení"@cs ;  rdfs:comment "Tento atribut zaznamenává odkaz na konkrétní úkon trestním řízení."@cs ;  rdfs:range crime-class:UkonVTrestnimRizeni ;  .# --------------------------- ÚKON V TRESTNÍM ŘÍZENÍ - PROVEDENO PODLE ----------------------------crime-prop:provedenoPodle_SekceZakona a rdf:Property ;  rdfs:label "provedeno podle - sekce zákona"@cs ;  rdfs:comment "Pro úkon v trestním řízení je tímto zaznamenán odkaz na sekci zákona, podle které byl úkon proveden. Sekcí zákona se rozumí paragraf, odstavec nebo písmeno."@cs ;  rdfs:subPropertyOf crime-prop:sekceZakona ;  rdfs:domain crime-class:UkonVTrestnimRizeni ;  rdfs:range crime-class:SekceZakona ;  .crime-prop:provedenoPodle_Paragraf a rdf:Property ;  rdfs:label "provedeno podle - sekce zákona - paragraf"@cs ;  rdfs:comment "Pro úkon v trestním řízení je tímto zaznamenán odkaz na příslušný paragraf sekce zákona, podle které byl úkon proveden."@cs ;  rdfs:subPropertyOf crime-prop:sekceZakona ;  rdfs:domain crime-class:UkonVTrestnimRizeni ;  rdfs:range crime-class:Paragraf ;  .crime-prop:provedenoPodle_Odstavec a rdf:Property ;  rdfs:label "provedeno podle - sekce zákona - odstavec"@cs ;  rdfs:comment "Pro úkon v trestním řízení je tímto zaznamenán odkaz na příslušný odstavec sekce zákona, podle které byl úkon proveden."@cs ;  rdfs:subPropertyOf crime-prop:sekceZakona ;  rdfs:domain crime-class:UkonVTrestnimRizeni ;  rdfs:range crime-class:Odstavec ;  .crime-prop:provedenoPodle_Pismeno a rdf:Property ;  rdfs:label "provedeno podle - sekce zákona - písmeno"@cs ;  rdfs:comment "Pro úkon v trestním řízení je tímto zaznamenán odkaz na příslušné písmeno sekce zákona, podle kterého byl úkon proveden."@cs ;  rdfs:subPropertyOf crime-prop:sekceZakona ;  rdfs:domain crime-class:UkonVTrestnimRizeni ;  rdfs:range crime-class:Pismeno ;  .# --------------------------- ÚKON V TRESTNÍM ŘÍZENÍ - ROZHODNE OKOLNOSTI ----------------------------crime-prop:rozhodneOkolnostiUvedeneV a rdf:Property ;  rdfs:subPropertyOf crime-prop:sekceZakona ;  rdfs:domain crime-class:PopisUkonuVTrestnimRizeni ;  .crime-prop:datumUkonuVTrestnimRizeni a rdf:Property ;  rdfs:label "datum provedení úkonu v trestním řízení"@cs ;  .